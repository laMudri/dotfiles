package oop;

interface Person {
    public void eat(String food);
}

class Student implements Person {
    private boolean mSleeping = false;
    
    @Override
    public void eat(String food) {
        if (!mSleeping)
            System.out.println("Student eats " + food);
    }

    public void sleep() {
        mSleeping = true;
        System.out.println("zzz");
    }

    public void wake() {
        Thread.sleep(10000);
        mSleeping = false;
        System.out.println("yawn");
    }

    public Student(boolean sleeping) {
        mSleeping = sleeping;
    }

    public Student() {
        mSleeping = true;
    }
}

class Ninja implements Person {
    @Override
    public void eat(String food) {
        System.out.println("Ninja eats " + food);
    }

    public void hide() {
        System.out.println("...");
    }
}

class NinjaStudent implements Person {
    
}

public class S2h {
    public static void main(String[] args) {

    }
}
