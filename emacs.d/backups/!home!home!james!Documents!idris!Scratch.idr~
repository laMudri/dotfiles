module Scratch
import Data.So
%default total
-- Nothing saved here
cong' : (f : _) -> x = y -> f x = f y
cong' f Refl = Refl

psrs : (m,n : _) -> S m + n = m + S n
psrs Z n = Refl
psrs (S m) n = let ih = psrs m n in cong' S ih

contrapositive : (a -> b) -> (Not b -> Not a)
contrapositive f x y = x (f y)

uncong : (f : a -> b) -> (g : a -> b) -> ((x : a) -> f x = g x) -> f = g
uncong f g p = ?uncong_rhs1

id' : a -> a

f : Show a => a -> String
f = g
  where
    g = show
