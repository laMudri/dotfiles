package code;

class A {
    public int a = 2;
    public void f() {
        System.out.println("A");
    }
}

class B extends A {
    public int a = 3;
    public void f() {
        System.out.println("B");
    }
}

public class S2e {
    public static void main(String[] args) {
        A x = new A();
        A y = new B();
        B z = new B();
        System.out.println(x.a);
        System.out.println(y.a);
        System.out.println(z.a);
        x.f();
        y.f();
        z.f();
    }
}
